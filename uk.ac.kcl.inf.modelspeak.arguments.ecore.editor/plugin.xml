<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>

<!--
-->

<plugin>

   <extension point="org.eclipse.ui.newWizards">
      <!-- @generated arguments -->
      <category
            id="org.eclipse.emf.ecore.Wizard.category.ID"
            name="%_UI_Wizard_category"/>
      <wizard
            id="uk.ac.kcl.inf.modelspeak.arguments.ecore.arguments.presentation.ArgumentsModelWizardID"
            name="%_UI_ArgumentsModelWizard_label"
            class="uk.ac.kcl.inf.modelspeak.arguments.ecore.arguments.presentation.ArgumentsModelWizard"
            category="org.eclipse.emf.ecore.Wizard.category.ID"
            icon="icons/full/obj16/ArgumentsModelFile.gif">
         <description>%_UI_ArgumentsModelWizard_description</description>
         <selection class="org.eclipse.core.resources.IResource"/>
      </wizard>
   </extension>

   <extension point="org.eclipse.ui.editors">
      <!-- @generated arguments -->
      <editor
            id="uk.ac.kcl.inf.modelspeak.arguments.ecore.arguments.presentation.ArgumentsEditorID"
            name="%_UI_ArgumentsEditor_label"
            icon="icons/full/obj16/ArgumentsModelFile.gif"
            extensions="arguments"
            class="uk.ac.kcl.inf.modelspeak.arguments.ecore.arguments.presentation.ArgumentsEditor"
            contributorClass="uk.ac.kcl.inf.modelspeak.arguments.ecore.arguments.presentation.ArgumentsActionBarContributor">
      </editor>
   </extension>

</plugin>
